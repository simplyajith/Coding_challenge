{
    "$schema": "http://json-schema.org/draft-06/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "maxItems": 1,
    "required": [
        "userId",
        "id",
        "title",
        "body"
    ],
    "properties": {
        "userId": {
            "$id": "#/properties/userId",
            "type": "integer"
        },
        "id": {
            "$id": "#/properties/id",
            "type": "integer"
        },
        "title": {
            "$id": "#/properties/title",
            "type": "string"
        },
        "body": {
            "$id": "#/properties/body",
            "type": "string"
        }
    }
}